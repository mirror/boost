
LIBNAME=regex++

CPP=@CXX@
AR=ar

CPP_PROJ=@CXXFLAGS@ -I../../../ -c 
	
ALL_O=@ac_all_o@
ALL_H=@ac_all_h@

lib$(LIBNAME).a : $(ALL_O)








c_regex_traits.o: $(ALL_H) ../src/c_regex_traits.cpp
	$(CPP) $(CPP_PROJ) ../src/c_regex_traits.cpp
	$(AR) -rc lib$(LIBNAME).a c_regex_traits.o


